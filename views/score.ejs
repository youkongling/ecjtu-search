<!DOCTYPE html>
<html>

<head>
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0" />
    <title>日新查询服务</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel='stylesheet' href='/stylesheets/jqbar.css' />
    <link rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.min.css" />
</head>

<body>
    <section class="col-xs-12 score-section">
        <div class="col-xs-2 score-back">
            <a href="javascript:window.history.go(-1);">
                <img class="img-responsive" src="/images/score-1.png" alt="" />
            </a>
        </div>
        <div class="col-xs-12 score-info">
            <div class="col-xs-7 score-user">
                <h2>刘小波</h2>
                <span>2015211001000622</span>
            </div>
            <div class="col-xs-2 col-xs-offset-3 score-login">
                <a href="/"><img class="img-responsive" src="/images/classList-2.png" alt="" /></a>
            </div>
        </div>
        <div class="col-xs-12 score-mask bars">
            <h2>本学期考试成绩</h2>
            <div class="col-xs-3 score-mask-item">
                <div id="bar-1"></div>
            </div>
            <div class="col-xs-3 score-mask-item">
                <div id="bar-2"></div>
            </div>
            <div class="col-xs-3 score-mask-item">
                <div id="bar-3"></div>
            </div>
            <div class="col-xs-3 score-mask-item">
                <div id="bar-4"></div>
            </div>
            <!--<div class="col-xs-3 score-mask-item">
                <div id="bar-5"></div>
            </div>
            <div class="col-xs-3 score-mask-item">
                <div id="bar-6"></div>
            </div>-->
        </div>
        <div class="col-xs-12 score-text">
            <div class="col-xs-10 col-xs-offset-1 score-text-item">
                <span>大学物理实验</span>
                <span>良好</span>
            </div>
            <div class="col-xs-10 col-xs-offset-1 score-text-item score-text-bad">
                <span>模拟电路实验</span>
                <span>不及格</span>
            </div>
        </div>
        <div class="col-xs-4 col-xs-offset-4 feedback score-feedback">
            <a href="/feedback">问题反馈</a>
        </div>
    </section>
    <script src="/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="/javascripts/jqbar.js"></script>
    <script src="/javascripts/index.js"></script>
    <script>
    $(document).ready(function() {
        $('#bar-1').jqbar({ label: '高等数学', value: 92, barColor: '#37CE22', orientation: 'v' });
        $('#bar-2').jqbar({ label: '模拟电子电路', value: 53, barColor: '#37CE22', orientation: 'v' });
        $('#bar-3').jqbar({ label: '马克思主义原理', value: 72, barColor: '#37CE22', orientation: 'v' });
        $('#bar-4').jqbar({ label: '大学英语(二)', value: 76, barColor: '#37CE22', orientation: 'v' });
        //          $('#bar-5').jqbar({ label: '线性代数' , value: 89, barColor: '#37CE22' ,orientation: 'v'});
        //          $('#bar-6').jqbar({ label: 'C语言编程' , value: 96, barColor: '#37CE22' ,orientation: 'v'});
    })
    </script>
</body>

</html>